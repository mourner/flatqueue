{
  "name": "flatqueue",
  "main": "index",
  "module": "index.mjs",
  "version": "1.0.0",
  "description": "The smallest and simplest JavaScript priority queue",
  "devDependencies": {
    "eslint": "^5.6.1",
    "eslint-config-mourner": "^3.0.0",
    "rollup": "^0.66.5",
    "tape": "^4.9.1",
    "tinyqueue": "^2.0.0"
  },
  "scripts": {
    "pretest": "eslint *.mjs",
    "test": "node --experimental-modules test.mjs",
    "bench": "node --experimental-modules bench.mjs",
    "build": "rollup -c",
    "prepublishOnly": "npm run build"
  },
  "eslintConfig": {
    "extends": "mourner",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "no-var": "error",
      "prefer-const": "error"
    }
  },
  "files": [
    "index.mjs",
    "index.js"
  ],
  "license": "ISC"
}
