{
  "name": "flatqueue",
  "version": "2.0.1",
  "description": "The smallest and simplest JavaScript priority queue",
  "type": "module",
  "main": "./index.cjs",
  "sideEffects": false,
  "exports": {
    "import": "index.js",
    "require": "index.cjs"
  },
  "types": "./index.d.ts",
  "devDependencies": {
    "eslint": "^8.12.0",
    "eslint-config-mourner": "^3.0.0",
    "rollup": "^2.70.1",
    "tinyqueue": "^2.0.3",
    "uvu": "^0.5.3"
  },
  "scripts": {
    "pretest": "eslint index.js test.js",
    "test": "node test.js",
    "build": "rollup index.js --file index.cjs --format umd --name FlatQueue",
    "prepublishOnly": "npm run build"
  },
  "eslintConfig": {
    "extends": "mourner"
  },
  "files": [
    "index.d.ts",
    "index.js",
    "index.cjs"
  ],
  "license": "ISC",
  "engines": {
    "node": ">= 12.17.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mourner/flatqueue.git"
  },
  "keywords": [
    "algorithms",
    "data structures",
    "priority queue",
    "binary heap"
  ],
  "author": "Vladimir Agafonkin"
}
